es. ", "cite_spans": [{"start": 37, "end": 39, "text": "1,", "ref_id": "BIBREF0"}, {"start": 40, "end": 41, "text": "2", "ref_id": "BIBREF1"}, {"start": 139, "end": 140, "text": "3", "ref_id": "BIBREF2"}, {"start": 458, "end": 459, "text": "4", "ref_id": "BIBREF3"}, {"start": 577, "end": 578, "text": "5", "ref_id": "BIBREF4"}], "ref_spans": [], "section": ""}], "bib_entries": {"BIBREF0": {"ref_id": "b0", "title": "ACE2 and Ang-(1-7) confer protection against development of diabetic retinopathy", "authors": [{"first": "A", "middle": [], "last": "Verma", "suffix": ""}], "year": 2012, "venue": "Mol Ther", "volume": "20", "issn": "", "pages": "28--36", "other_ids": {}}, "BIBREF1": {"ref_id": "b1", "title": "Macrophage migration inhibitory factor in hypothalamic paraventricular nucleus neurons decreases blood pressure in spontaneously hypertensive rats", "authors": [{"first": "H", "middle": [], "last": "Li", "suffix": ""}], "year": 2008, "venue": "Faseb J", "volume": "22", "issn": "", "pages": "3175--3185", "other_ids": {}}, "BIBREF2": {"ref_id": "b2", "title": "Cloning and characterization of a secreted form of angiotensin-converting enzyme 2", "authors": [{"first": "M", "middle": ["J"], "last": "Huentelman", "suffix": ""}, {"first": "J", "middle": [], "last": "Zubcevic", "suffix": ""}, {"first": "M", "middle": ["J"], "last": "Katovich", "suffix": ""}, {"first": "M", "middle": [], "last": "Raizada", "suffix": ""}], "year": 2004, "venue": "Regul Pept", "volume": "122", "issn": "", "pages": "61--67", "other_ids": {}}, "BIBREF3": {"ref_id": "b3", "title": "High-efficiency transduction of the mouse retina by tyrosine-mutant AAV serotype vectors", "authors": [{"first": "H", "middle": [], "last": "Petrs-Silva", "suffix": ""}], "year": 2009, "venue": "Mol Ther", "volume": "17", "issn": "", "pages": "463--471", "other_ids": {}}, "BIBREF4": {"ref_id": "b4", "title": "Recombinant adeno-associated virus purification using novel methods improves infectious titer and yield", "author