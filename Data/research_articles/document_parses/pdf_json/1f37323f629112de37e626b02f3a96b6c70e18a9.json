    "metadata": {
        "title": "A recommendation for IVF lab practice in light of the current COVID-19 pandemic",
        "authors": [
            {
                "first": "Amir",
                "middle": [],
                "last": "Arav",
                "suffix": "",
                "affiliation": {
                    "laboratory": "",
